    xmlns:ttva="http://skynav.com/ns/ttv/annotations" ttva:warnOn="all" ttva:expectedErrors="0" ttva:expectedWarnings="2">
  <head>
    <profile xmlns="http://www.w3.org/ns/ttml#parameter">
      <extensions>
        <extension>http://example.com/ns/ttml/extension/#foo</extension>
      </extensions>
    </profile>
  </head>
</tt>
