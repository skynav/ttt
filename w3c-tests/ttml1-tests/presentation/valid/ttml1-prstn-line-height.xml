<?xml version="1.0" encoding="UTF-8"?>
<tt xml:lang="en"
    xmlns="http://www.w3.org/ns/ttml"
    xmlns:ttm="http://www.w3.org/ns/ttml#metadata"
    xmlns:ttp="http://www.w3.org/ns/ttml#parameter" ttp:profile="dfxp-presentation"
    xmlns:tts="http://www.w3.org/ns/ttml#styling"
    xmlns:ttva="http://skynav.com/ns/ttv/annotations" ttva:warnOn="all" ttva:expectedErrors="0" ttva:expectedWarnings="0" ttva:model="ttml1">
  <head>
    <metadata> 
      <ttm:desc>Test tts:lineHeight attribute using "normal".</ttm:desc>
    </metadata>
    <layout>
       <region xml:id="bottom" tts:origin="10% 10%" tts:extent="80% 80%" />
    </layout>
  </head>
  <body tts:fontSize="2c">
    <div>
    	<p begin="0s" end="10s" region="bottom">The line height of this<br/>two-row caption is "normal".</p>
    </div>
  </body>
</tt>
