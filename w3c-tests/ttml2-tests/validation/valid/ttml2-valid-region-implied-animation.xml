<?xml version="1.0" encoding="utf-8"?>
<!-- valid - use of all styles on div and p that imply animation of associated region -->
<tt xml:lang="" xmlns="http://www.w3.org/ns/ttml"
    xmlns:ttp="http://www.w3.org/ns/ttml#parameter" ttp:profile="ttml2-transformation"
    xmlns:tts="http://www.w3.org/ns/ttml#styling" tts:extent="1280px 720px"
    xmlns:ttva="http://skynav.com/ns/ttv/annotations" ttva:warnOn="all" ttva:expectedErrors="0" ttva:expectedWarnings="0" ttva:model="ttml2">
  <head>
    <layout>
      <region xml:id="r1" tts:extent="100px 100px" tts:position="center"/>
    </layout>
  </head>
  <body region="r1">
    <div tts:disparity="1px"/>
    <div tts:extent="1px 1px"/>
    <div tts:origin="1px 1px"/>
    <div tts:position="left 1px top 1px"/>
    <div>
      <p tts:disparity="1px"/>
      <p tts:extent="1px 1px"/>
      <p tts:origin="1px 1px"/>
      <p tts:position="left 1px top 1px"/>
    </div>
  </body>
</tt>
