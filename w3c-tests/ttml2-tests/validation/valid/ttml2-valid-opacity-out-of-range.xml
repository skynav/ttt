<?xml version="1.0" encoding="utf-8"?>
<!-- valid - tts:opacity - #opacity values out of range (but still valid) - should produce 5 warnings -->
<tt xml:lang="en" xmlns="http://www.w3.org/ns/ttml"
    xmlns:ttp="http://www.w3.org/ns/ttml#parameter" ttp:profile="ttml2-transformation"
    xmlns:tts="http://www.w3.org/ns/ttml#styling"
    xmlns:ttva="http://skynav.com/ns/ttv/annotations" ttva:warnOn="all" ttva:expectedErrors="0" ttva:expectedWarnings="5" ttva:model="ttml2">
  <head>
    <styling>
      <style tts:opacity="NaN"/>
      <style tts:opacity="-INF"/>
      <style tts:opacity="-.0000001"/>
      <style tts:opacity="1.0000001"/>
      <style tts:opacity="INF"/>
    </styling>
  </head>
  <body/>
</tt>


