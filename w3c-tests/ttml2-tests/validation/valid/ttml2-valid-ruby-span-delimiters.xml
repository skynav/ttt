<?xml version="1.0" encoding="utf-8"?>
<!-- valid - tts:ruby - maximal ruby content model on spans - without shortcuts, with delimiters -->
<tt xml:lang="en" xmlns="http://www.w3.org/ns/ttml"
    xmlns:ttp="http://www.w3.org/ns/ttml#parameter" ttp:profile="ttml2-transformation"
    xmlns:tts="http://www.w3.org/ns/ttml#styling"
    xmlns:ttva="http://skynav.com/ns/ttv/annotations" ttva:warnOn="all" ttva:expectedErrors="0" ttva:expectedWarnings="0" ttva:model="ttml2">
  <head/>
<body>
  <div>
    <p>
      <span tts:ruby="container">
        <span tts:ruby="baseContainer">
          <span tts:ruby="base"/>
          <span tts:ruby="base"/>
        </span>
        <span tts:ruby="textContainer" tts:rubyPosition="before">
          <span tts:ruby="delimiter"/>
          <span tts:ruby="text"/>
          <span tts:ruby="text"/>
          <span tts:ruby="delimiter"/>
        </span>
        <span tts:ruby="textContainer" tts:rubyPosition="after">
          <span tts:ruby="delimiter"/>
          <span tts:ruby="text"/>
          <span tts:ruby="text"/>
          <span tts:ruby="delimiter"/>
        </span>
      </span>
    </p>
  </div>
</body>
</tt>
