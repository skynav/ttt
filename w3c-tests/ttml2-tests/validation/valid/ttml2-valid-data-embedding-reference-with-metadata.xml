  <head>
    <resources>
      <data xml:id="data1" src="http://www.w3.org/ns/ttml/resource/#empty" type="application/octet-stream">
        <metadata/>
      </data>
      <data xml:id="data2" src="http://www.w3.org/ns/ttml/resource/#empty" type="application/octet-stream">
        <metadata xmlns:ttm="http://www.w3.org/ns/ttml#metadata">
