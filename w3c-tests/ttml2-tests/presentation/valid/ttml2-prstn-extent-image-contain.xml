<?xml version="1.0" encoding="UTF-8"?>
<tt xml:lang="en"
    xmlns="http://www.w3.org/ns/ttml"
    xmlns:ttm="http://www.w3.org/ns/ttml#metadata"
    xmlns:ttp="http://www.w3.org/ns/ttml#parameter"
    xmlns:tts="http://www.w3.org/ns/ttml#styling"
    ttp:profile="ttml2-presentation"
    tts:extent="1280px 720px"
    xmlns:ttva="http://skynav.com/ns/ttv/annotations" ttva:warnOn="all" ttva:expectedErrors="0" ttva:expectedWarnings="0" ttva:model="ttml2">
  <head>
    <metadata>
      <ttm:desc>#extent-contain (image)</ttm:desc>
    </metadata>
    <layout>
      <region xml:id="area1" tts:extent="720px 360px" tts:position="center" tts:backgroundColor="#A0A0A0" tts:displayAlign="center"/>
    </layout>
  </head>
  <body>
    <div region="area1" begin="0s" end="1s">
      <p tts:textAlign="center">
        <image src="../resources/images/ttml2-prstn-image.png" type="image/png" tts:extent="contain"/>
      </p>
    </div>
  </body>
</tt>
