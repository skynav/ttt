<?xml version="1.0" encoding="UTF-8"?>
<tt xml:lang="en"
    xmlns="http://www.w3.org/ns/ttml"
    xmlns:ttm="http://www.w3.org/ns/ttml#metadata"
    xmlns:ttp="http://www.w3.org/ns/ttml#parameter"
    xmlns:tta="http://www.w3.org/ns/ttml#audio"
    ttp:profile="ttml2-presentation"
    xmlns:ttva="http://skynav.com/ns/ttv/annotations" ttva:warnOn="all" ttva:expectedErrors="0" ttva:expectedWarnings="0" ttva:model="ttml2">
  <head>
  </head>
  <body>
    <div>
      <p begin="0s" end="3s">
        <span tta:speak="normal" tta:pitch="0">This sentence is spoken normally.</span>
      </p>        
      <p begin="3s" end="6s">
        <span tta:speak="normal" tta:pitch="-10%">This sentence should be deeper.</span>
      </p>
      <p begin="6s" end="9s">
        <span tta:speak="normal" tta:pitch="+10%">This sentence should be higher.</span>
      </p>
    </div>
  </body>
</tt>


