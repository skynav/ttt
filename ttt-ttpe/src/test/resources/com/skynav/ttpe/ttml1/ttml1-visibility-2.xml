<?xml version="1.0" encoding="utf-8"?>
<!-- @tts:visibility on region - should render 1 instances of 'visible', and 0 instances of 'hidden -->
<tt xml:lang="en" xmlns="http://www.w3.org/ns/ttml"
    xmlns:tts="http://www.w3.org/ns/ttml#styling"
    xmlns:ttva="http://skynav.com/ns/ttv/annotations" ttva:warnOn="all" ttva:expectedErrors="0" ttva:expectedWarnings="0"
    ttva:noWarnOn="missing-profile" ttva:model="ttml1">
  <head>
    <layout>
      <region xml:id="r0" tts:extent="80% 20%" tts:origin="10% 10%" tts:textAlign="center" tts:visibility="visible"/>
      <region xml:id="r1" tts:extent="80% 20%" tts:origin="10% 70%" tts:textAlign="center" tts:visibility="hidden"/>
    </layout>
  </head>
  <body begin="0s" end="1s" tts:fontFamily="Noto Sans" tts:fontSize="24px">
    <div region="r0">
      <p>
        <span>visible</span>
      </p>
    </div>
    <div region="r1">
      <p>
        <span>hidden</span>
      </p>
    </div>
  </body>
</tt>
